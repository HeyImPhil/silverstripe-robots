{
    "name": "heyimphil/silverstripe-robotson",
    "description": "A robots.txt module for SilverStripe.",
    "type": "silverstripe-vendormodule",
    "keywords": ["silverstripe","CMS","robots","bots"],
    "license": "BSD-3-Clause",
    "require": {
        "php": ">7.3",
        "silverstripe/cms": "^4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7",
        "jdolba/silverstripe-coding-standards": "^0.1.1"
    },
    "autoload": {
        "psr-4": {
            "Heyimphil\\Robotson\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Heyimphil\\Robotson\\Tests\\": "tests/php/"
        }
    }
}
